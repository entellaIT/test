 
@{
    Layout = null;
    ViewBag.Title = "Welcome to Videoburst";
    ViewBag.GACode = System.Configuration.ConfigurationManager.AppSettings["GACode"] != null ? System.Configuration.ConfigurationManager.AppSettings["GACode"] : "UA-59804226-1";
}

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <!-- The above 3 meta tags *must* come first in the head; any other head content must come *after* these tags -->
    <meta name="description" content="">
    <meta name="author" content="">
    <link rel="icon" href="~/images/favicon.ico">

    <title>@ViewBag.Title</title>

   
    <!-- Bootstrap core CSS -->
    @Styles.Render("~/Content/themes/fastout/css/bootstrap")
    <!-- Custom styles for this template -->
    @Styles.Render("~/Content/themes/fastout/css/custom")
    @Styles.Render("~/Content/themes/fastout/css/cover")
    @Styles.Render("~/Content/themes/fastout/css/flexslider")
    


    @Styles.Render("~/Content/themes/fastout/css/timeline")
    @Styles.Render("~/Content/themes/fastout/css/owl.carousel")
    @Styles.Render("~/Content/themes/fastout/css/owl.theme")

    <!-- Just for debugging purposes. Don't actually copy these 2 lines! -->
    <!--[if lt IE 9]><script src="../Scripts/ie8-responsive-file-warning.js"></script><![endif]-->
    @*<script src='@Url.Content("~/Content/themes/fastout/js/ie-emulation-modes-warning.js")'></script>*@

    @Html.IncludeVersionedJs(@Url.Content("~/Scripts/ie-emulation-modes-warning.js"))

    <!-- HTML5 shim and Respond.js for IE8 support of HTML5 elements and media queries -->
    <!--[if lt IE 9]>
      <script src="https://oss.maxcdn.com/html5shiv/3.7.2/html5shiv.min.js"></script>
      <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>

    <![endif]-->

    <style type="text/css">
        video
        {
            position: fixed;
            top: 50%;
            left: 50%;
            min-width: 100%;
            min-height: 100%;
            width: auto;
            height: auto;
            z-index: -100;
            transform: translateX(-50%) translateY(-50%);
        }

        body {
            background: none;
        }
    </style>

    @Html.IncludeVersionedJs(@Url.Content("~/Scripts/jquery-2.1.1.min.js"))
    @Html.IncludeVersionedJs(@Url.Content("~/Content/themes/fastout/js/bootstrap.min.js")) 
    @Html.IncludeVersionedJs(@Url.Content("~/Scripts/generic.js"))
</head>

<body>
    @RenderSection("scriptstopbody", required: false)
    
        <video id="bgVideo" controls preload="true" autoplay loop muted>
            <source src='@Url.Content("~/loop-videos/video.mp4")' type="video/mp4">
        </video>
        <nav class="navbar navbar-inverse navbar-fixed-top homeTopBar">
            <div class="container home-container">
                <div class="row">
                    <div class="col-sm-5 homeNavHeader">
                        <div class="navbar-header homeNavHeader dropdown">
                            <button type="button" class="navbar-toggle collapsed iconButton01" data-toggle="collapse" data-target="#navbar" aria-expanded="false" aria-controls="navbar">
                                <span class="sr-only">Toggle navigation</span>
                                <span class="icon-bar toggleIcon"></span>
                                <span class="icon-bar toggleIcon"></span>
                                <span class="icon-bar toggleIcon"></span>
                            </button>
                            <a class="navbar-brand homeLogo" href="javascript:void(0)">
                                <img src='@Url.Content("~/Content/themes/fastout/images/logo.png")' class="img-responsive" alt="">
                            </a>
                        </div>
                    </div>
                    <div class="col-sm-7 text-right homeNavHeader">
                        <!-- <div id="navbar" class="navbar-collapse collapse">-->
                        <div id="navbar" class="nav navbar-nav navbar-collapse collapse " style="width:100%;">  
                            @Html.Partial("_DashboardLoginPartial", new VideoBurstWeb.Models.VBUserProfile())
                        </div>
                    </div>
                    <!--/.navbar-collapse -->
                </div>
            </div>
        </nav>
    <div class="site-wrapper">
        <div class="site-wrapper-inner">
            <div class="home-container">

                <!-- Body HTML Start here
                ================================================== -->
                @RenderBody()

            </div>
        </div>
    </div>

        @Html.Partial("_Footer")
        <!-- Bootstrap core JavaScript
        ================================================== -->
        @*<script src='@Url.Content("~/Scripts/jquery-2.1.1.min.js")'></script>
            <script src='@Url.Content("~/Content/themes/fastout/js/bootstrap.min.js")'></script>
            <script src='@Url.Content("~/Scripts/generic.js")'></script>*@

       


        <!-- Just to make our placeholder images work. Don't actually copy the next line! -->
        @*<script src='@Url.Content("~/Content/themes/fastout/js/vendor/holder.min.js")'></script>*@
        @Html.IncludeVersionedJs(@Url.Content("~/Scripts/holder.min.js"))

        <!-- IE10 viewport hack for Surface/desktop Windows 8 bug -->
        @*<script src='@Url.Content("~/Content/themes/fastout/js/ie10-viewport-bug-workaround.js")'></script>*@
        @Html.IncludeVersionedJs(@Url.Content("~/Scripts/ie10-viewport-bug-workaround.js"))


        <!-- FlexSlider -->
        @*<script defer src='@Url.Content("~/Content/themes/fastout/js/jquery.flexslider.js")'></script>*@
        @Html.IncludeVersionedJs(@Url.Content("~/Content/themes/fastout/js/jquery.flexslider.js"))

        <script type="text/javascript">
        $(function () {
            //SyntaxHighlighter.all();
        });
        $(window).load(function () {
            $('.flexslider').flexslider({
                animation: "slide",
                animationLoop: false,
                itemWidth: 280,
                itemMargin: 5,
                minItems: 2,
                maxItems: 25,
                start: function (slider) {
                    $('body').removeClass('loading');
                }
            });
        });
        </script>

        <!-- Optional FlexSlider Additions -->
        @*<script src='@Url.Content("~/Content/themes/fastout/js/jquery.easing.js")'></script>*@
        @Html.IncludeVersionedJs(@Url.Content("~/Content/themes/fastout/js/jquery.easing.js"))

        @RenderSection("scriptsbottombody", required: false)
        @RenderSection("scriptsbottombody2", required: false)


        <script type="text/javascript">
        $(document).ready(function () {
            @if (@TempData.ContainsKey("alertmessage") && !String.IsNullOrEmpty(Convert.ToString(@TempData["alertmessage"])))
            {
                @Html.Raw("alertBox('" + @TempData["alertmessage"] + "');")
            }
            @if (@TempData.ContainsKey("confirmmessage") && !String.IsNullOrEmpty(Convert.ToString(@TempData["confirmmessage"])))
            {
                @Html.Raw(@TempData["confirmmessage"])
            }
        });
        </script>
        @Html.Partial("_PopupsPartial")
        <script type="text/javascript">
        (function (i, s, o, g, r, a, m) {
            i['GoogleAnalyticsObject'] = r; i[r] = i[r] || function () {
                (i[r].q = i[r].q || []).push(arguments)
            }, i[r].l = 1 * new Date(); a = s.createElement(o),
            m = s.getElementsByTagName(o)[0]; a.async = 1; a.src = g; m.parentNode.insertBefore(a, m)
        })(window, document, 'script', '//www.google-analytics.com/analytics.js', 'ga');

        ga('create', '@ViewBag.GACode', 'auto');
        ga('send', 'pageview');
        </script>
</body>
</html>
